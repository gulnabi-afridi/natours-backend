{
    "extends": ["airbnb","prettier","plugin:node/recommended"], 
    "plugin":["prettier"], 
    "rules": {
        "prettier/prettier":"error",
        "no-unused-vars":"warn",
        "no-console":"warn",
        "func-names":"off",
        "no-process-exit":"off",
        "class-methods-use-this":"off"
    }
}
